{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-assessment11"
		},
		"SqlServer1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SqlServer1'"
		},
		"db_to_snowflake_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'db_to_snowflake'"
		},
		"local_server_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'local_server'"
		},
		"SqlServer1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "DESKTOP-SDMB9NV"
		},
		"SqlServer1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "ride-sharing"
		},
		"SqlServer1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sa"
		},
		"blob_connection_sasUri": {
			"type": "secureString",
			"metadata": "Secure string for 'sasUri' of 'blob_connection'"
		},
		"db_to_snowflake_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "RIDE_SHARING_DB"
		},
		"db_to_snowflake_properties_typeProperties_role": {
			"type": "string",
			"defaultValue": "ACCOUNTADMIN"
		},
		"db_to_snowflake_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "LZLTXPB-FW41340.snowflakecomputing.com"
		},
		"linkedService1_sasUri": {
			"type": "secureString",
			"metadata": "Secure string for 'sasUri' of 'linkedService1'"
		},
		"local_server_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "DESKTOP-SDMB9NV"
		},
		"local_server_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "ride-sharing"
		},
		"local_server_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "kavin11"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-assessment11_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"local_server_password": {
						"value": "[parameters('local_server_password')]"
					},
					"SqlServer1_password": {
						"value": "[parameters('SqlServer1_password')]"
					},
					"db_to_snowflake_password": {
						"value": "[parameters('db_to_snowflake_password')]"
					},
					"blob_connection_sasUri": {
						"value": "[parameters('blob_connection_sasUri')]"
					},
					"linkedService1_sasUri": {
						"value": "[parameters('linkedService1_sasUri')]"
					},
					"local_server_properties_typeProperties_server": {
						"value": "[parameters('local_server_properties_typeProperties_server')]"
					},
					"local_server_properties_typeProperties_database": {
						"value": "[parameters('local_server_properties_typeProperties_database')]"
					},
					"local_server_properties_typeProperties_userName": {
						"value": "[parameters('local_server_properties_typeProperties_userName')]"
					},
					"SqlServer1_properties_typeProperties_server": {
						"value": "[parameters('SqlServer1_properties_typeProperties_server')]"
					},
					"SqlServer1_properties_typeProperties_database": {
						"value": "[parameters('SqlServer1_properties_typeProperties_database')]"
					},
					"SqlServer1_properties_typeProperties_userName": {
						"value": "[parameters('SqlServer1_properties_typeProperties_userName')]"
					},
					"db_to_snowflake_properties_typeProperties_database": {
						"value": "[parameters('db_to_snowflake_properties_typeProperties_database')]"
					},
					"db_to_snowflake_properties_typeProperties_role": {
						"value": "[parameters('db_to_snowflake_properties_typeProperties_role')]"
					},
					"db_to_snowflake_properties_typeProperties_host": {
						"value": "[parameters('db_to_snowflake_properties_typeProperties_host')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}